{
  "name": "@socialgouv/datafiller-data",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.20.0",
  "main": "dist/index.js",
  "files": [
    "data",
    "dist",
    "src"
  ],
  "homepage": "https://github.com/SocialGouv/datafiller-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/datafiller-data.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_OPTIONS=--insecure-http-parser ts-node src/index.ts",
    "dev": "tsc --watch",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "p-pipe": "^3.1.0",
    "remark": "^12.0.1",
    "remark-html": "^13.0.1"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "^1.63.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.23.0",
    "esm": "^3.2.25",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.9"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "run-p lint test"
    }
  }
}
