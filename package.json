{
  "name": "@socialgouv/datafiller-data",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "main": "index.js",
  "files": [
    "data"
  ],
  "homepage": "https://github.com/SocialGouv/datafiller-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/datafiller-data.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_OPTIONS=--insecure-http-parser ts-node src/index.ts",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "p-pipe": "^3.1.0",
    "remark": "^12.0.1",
    "remark-html": "^12.0.0"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "^1.23.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^13.13.5",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.6.0",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts",
      "pre-push": "run-p lint test"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
